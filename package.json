{
  "name": "djantajs-bundle-composer",
  "description": "djantajs package builder core bundle",
  "homepage": "https://djantajs.io",
  "version": "0.1.3",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=4.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/djanta/djantajs-bundle-composer.git"
  },
  "keywords": [
    "compose",
    "djantajs",
    "djanta",
    "bundle",
    "manifest",
    "packaging",
    "package",
    "assembly",
    "assembler"
  ],
  "author": "djantajs.team@djantajs.io",
  "contributors": [
    "Stanislas KOFFI ASSOUTOVI <stanislaska@djantajs.io> (https://github.com/stanislaska)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djanta/djantajs-bundle-composer/issues"
  },
  "snyk": true,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "precompile": "npm run clean",
    "clean": "./node_modules/.bin/rimraf .djanta-rc.json && npm run clean:test && npm run clean:cache",
    "clean:test": "./node_modules/.bin/rimraf ./test/.nyc_output ./test/.coverage ./test/.eslint ./test/.mochawesome",
    "clean:cache": "./node_modules/.bin/rimraf ./node_modules/.cache/nyc ./node_modules/.cache/@babel ./node_modules/.cache/babel-register",
    "compile": "echo compiling ...",
    "build": "#npm run precompile && npm run compile",
    "prepublish": "npm run compile && npm run build && npm run test",
    "test": "npm run test:init && npm run snyk:test && npm run test:nyc && npm run lint:eslint",
    "test:init": "./node_modules/.bin/cross-env NODE_ENV=test && npm run compile",
    "test:mocha": "./node_modules/.bin/_mocha --recursive --reporter text --reporter mochawesome --reporter-options reportDir=./test/.mochawesome -- test/**/**/*.spec.js",
    "test:nyc": "./node_modules/.bin/nyc --all --clean -- npm run test:mocha",
    "lint:eslint": "./node_modules/.bin/eslint --color --ext .js -f html -o ./test/.eslint/lint-results.html .",
    "coverage": "npm run coverage:coveralls && npm run coverage:codeclimate",
    "coverage:codeclimate": "./node_modules/.bin/nyc report --reporter=lcov | ./node_modules/.bin/codeclimate-test-reporter < ./test/.coverage/lcov.info",
    "coverage:coveralls": "./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/coveralls/bin/coveralls.js",
    "prepare": "#npm run snyk:protect",
    "snyk:test": "./node_modules/.bin/snyk test --file=./package.json",
    "snyk:publish": "./node_modules/.bin/snyk wizard --file=./package.json",
    "snyk:monitor": "./node_modules/.bin/snyk monitor --file=./package.json",
    "snyk:login": "./node_modules/.bin/snyk auth",
    "snyk:protect": "./node_modules/.bin/snyk protect"
  },
  "bin": {
    "djantajs-bundle": "./bin/cli.js"
  },
  "main": "index.js",
  "dependencies": {
    "semver": "^5.5.0",
    "mkdirp": "^0.5.1",
    "lodash": "^4.17.10",
    "chalk": "^2.4.1",
    "command-line-args": "^5.0.2",
    "command-line-usage": "^5.0.5",
    "inquirer": "^6.2.0",
    "snyk": "^1.82.1",
    "inquirer-autocomplete-prompt": "^1.0.1"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.2",
    "rimraf": "^2.6.2",
    "chai": "^4.1.2",
    "chai-json-equal": "^0.0.1",
    "chai-like": "^1.1.1",
    "eslint-config-djantajs": "^1.1.1",
    "eslint": "^4.19.1",
    "coveralls": "^3.0.1",
    "codeclimate-test-reporter": "^0.5.0",
    "nyc": "^12.0.2",
    "cross-env": "^5.2.0",
    "babel-plugin-istanbul": "^4.1.6",
    "inquirer-test": "^2.0.1",
    "@babel/cli": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/core": "^7.0.0"
  }
}
