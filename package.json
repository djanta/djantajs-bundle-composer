{
  "name": "djantajs-runtime-sauce",
  "description": "djantajs package builder core bundle",
  "version": "0.0.0",
  "engines": {
    "node": ">=7.0.0",
    "npm": ">=4.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/djanta/djantajs-runtime-sauce.git"
  },
  "keywords": [
    "sauce",
    "djantajs",
    "djanta",
    "bundle",
    "manifest",
    "packaging",
    "package"
  ],
  "author": "djantajs.team@djanta.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djanta/djantajs-runtime-sauce/issues"
  },
  "homepage": "https://github.com/djanta/djantajs-runtime-sauce#readme",
  "scripts": {
    "precompile": "npm run clean",
    "clean": "rimraf .djanta-rc.json",
    "compile": "grunt compile",
    "build": "npm run precompile && npm run compile",
    "test": "grunt compile && npm run coverage",
    "coverage": "./node_modules/.bin/_mocha ./test/**/*.spec.js --reporter mochawesome --reporter-options reportDir=./test/report,reportFilename=result --recursive --timeout 18000 --expose-gc --colors"
  },
  "dependencies": {
    "string": "^3.3.3",
    "dynavers": "^0.2.0",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "djantajs-runtime": "git+ssh://git@github.com/djanta/djantajs-runtime.git#v0.1.0",
    "grunt": "^1.0.1",
    "grunt-available-tasks": "^0.6.3",
    "grunt-concurrent": "^2.3.1",
    "grunt-djantajs-compiler": "git+ssh://git@github.com/djanta/grunt-djantajs-compiler.git#develop",
    "load-grunt-tasks": "^3.5.2",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "mochawesome": "^3.0.1",
    "rimraf": "^2.6.2"
  }
}
