{
  "name": "djantajs-compose",
  "description": "djantajs package builder core bundle",
  "version": "0.1.3",
  "engines": {
    "node": ">=7.0.0",
    "npm": ">=4.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/djanta/djantajs-compose.git"
  },
  "keywords": [
    "compose",
    "djantajs",
    "djanta",
    "bundle",
    "manifest",
    "packaging",
    "package",
    "assembly"
  ],
  "author": "djantajs.team@djanta.io",
  "contributors": [
    "Stanislas KOFFI ASSOUTOVI <stanislaska@djantajs.io> (https://github.com/stanislaska)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djanta/djantajs-compose/issues"
  },
  "homepage": "https://github.com/djanta/djantajs-compose#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "precompile": "npm run clean",
    "clean": "rimraf .djanta-rc.json",
    "compile": "echo .",
    "build": "npm run precompile && npm run compile",
    "test": "npm run coverage",
    "lint": "./node_modules/.bin/eslint --color --ext .js -f html -o ./reports/lint-results.html .",
    "hint": "./node_modules/.bin/jshint --verbose --config .jshintrc --reporter jslint",
    "coverage": "./node_modules/.bin/_mocha ./test/**/**/*.spec.js --reporter mochawesome --reporter-options reportDir=./test/report,reportFilename=result --recursive --timeout 18000 --expose-gc --colors",
    "cover": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec ./test/**/**/*.spec.js",
    "coveralls-publish": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "codeclimate-publish": "./node_modules/mocha/bin/codeclimate-test-reporter < ./coverage/lcov.info",
    "snyk": "./node_modules/.bin/snyk auth && ./node_modules/.bin/snyk test --file=./package.json"
  },
  "bin": {
    "djantajs-compose": "./bin/cli.js"
  },
  "main": "index.js",
  "dependencies": {
    "string": "^3.3.3",
    "dynavers": "^0.2.0",
    "semver": "^5.5.0",
    "mkdirp": "^0.5.1",
    "lodash": "^4.17.10",
    "chalk": "^2.4.1",
    "command-line-args": "^5.0.2",
    "npm-registry": "^0.1.13",
    "npm-registry-client": "^8.5.1"
  },
  "devDependencies": {
    "mocha": "^4.0.1",
    "mochawesome": "^3.0.1",
    "rimraf": "^2.6.2",
    "chai": "^4.1.2",
    "chai-json-equal": "^0.0.1",
    "chai-like": "^1.1.1",
    "eslint-config-djantajs": "^1.0.0",
    "eslint": "^4.19.1",
    "coveralls": "^3.0.1",
    "istanbul": "^0.4.5",
    "codeclimate-test-reporter": "^0.5.0",
    "snyk": "^1.82.1"
  }
}
