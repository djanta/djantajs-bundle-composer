{
  "name": "djantajs-compose",
  "description": "djantajs package builder core bundle",
  "version": "0.1.3",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=4.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/djanta/djantajs-compose.git"
  },
  "keywords": [
    "compose",
    "djantajs",
    "djanta",
    "bundle",
    "manifest",
    "packaging",
    "package",
    "assembly"
  ],
  "author": "djantajs.team@djanta.io",
  "contributors": [
    "Stanislas KOFFI ASSOUTOVI <stanislaska@djantajs.io> (https://github.com/stanislaska)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djanta/djantajs-compose/issues"
  },
  "homepage": "https://github.com/djanta/djantajs-compose#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "precompile": "npm run clean",
    "clean": "rimraf .djanta-rc.json .nyc_output coverage reports",
    "compile": "echo compiling ...",
    "build": "npm run precompile && npm run compile",
    "test": "npm run snyk && npm run lint && npm run mocha",
    "lint": "./node_modules/.bin/eslint --color --ext .js -f html -o ./reports/lint-results.html .",
    "mocha": "npm run compile && mkdir ./.nyc_output && ./node_modules/.bin/nyc --reporter=lcov --reporter=html --reporter=text ./node_modules/.bin/_mocha -- ./test/**/**/*.spec.js",
    "coveralls": "./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/coveralls/bin/coveralls.js",
    "codeclimate": "./node_modules/.bin/nyc report --reporter=lcov | ./node_modules/.bin/codeclimate-test-reporter < ./coverage/lcov.info",
    "coverage": "npm run-script coveralls && npm run-script codeclimate",
    "snyk": "./node_modules/.bin/snyk test --file=./package.json",
    "snyk-login": "./node_modules/.bin/snyk auth",
    "snyk-monitor": "./node_modules/.bin/snyk monitor --file=./package.json",
    "snyk-publish": "./node_modules/.bin/snyk wizard --file=./package.json"
  },
  "bin": {
    "djantajs-compose": "./bin/cli.js"
  },
  "main": "index.js",
  "dependencies": {
    "semver": "^5.5.0",
    "mkdirp": "^0.5.1",
    "lodash": "^4.17.10",
    "chalk": "^2.4.1",
    "command-line-args": "^5.0.2",
    "command-line-usage": "^5.0.5"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.2",
    "rimraf": "^2.6.2",
    "chai": "^4.1.2",
    "chai-json-equal": "^0.0.1",
    "chai-like": "^1.1.1",
    "eslint-config-djantajs": "^1.1.1",
    "eslint": "^4.19.1",
    "coveralls": "^3.0.1",
    "codeclimate-test-reporter": "^0.5.0",
    "snyk": "^1.82.1",
    "nyc": "^12.0.2"
  }
}
