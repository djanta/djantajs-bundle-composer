{
  "check-coverage": false,
  "per-file": true,
  "lines": 99,
  "functions": 99,
  "branches": 99,
  "statements": 99,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  },
  "reporter": [
    "html",
    "text",
    "lcov",
    "text-summary"
  ],
  "extension": [
    ".js"
  ],
  "cache": true,
  "all": true,
  "show-process-tree": true,
  "temp-directory": "./test/.nyc_output",
  "report-dir": "./test/.coverage"
}
