{
  "description": "Nyc based default coverage configuration",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "watermarks": {
      "lines": [80, 95],
      "functions": [80, 95],
      "branches": [80, 95],
      "statements": [80, 95]
    },
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "build"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "clover",
      "html"
    ],
    "require": [
      "./test/helpers/some-helper.js"
    ],
    "extension": [
      ".jsx",
      ".js"
    ],
    "cache": true,
    "all": true,
    "temp-directory": "./alternative-tmp",
    "report-dir": "./alternative"
  }
}
