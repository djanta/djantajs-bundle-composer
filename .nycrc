{
  "description": "Nyc based default coverage configuration",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "build"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "clover",
      "html"
    ],
    "extension": [
      ".jsx",
      ".js"
    ],
    "cache": true,
    "all": true,
    "temp-directory": "./alternative-tmp",
    "report-dir": "./alternative",
    "sourceMap": false,
    "instrument": false
  }
}
